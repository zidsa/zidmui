{
  "name": "@zidsa/zidmui",
  "type": "module",
  "version": "1.1.1",
  "description": "Zid MUI shared components and utilities",
  "author": "Abdulaziz Homaily <a.homaily@gmail.com>",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/zidsa/zidmui#readme",
  "repository": "https://github.com/zidsa/zidmui",
  "packageManager": "pnpm@9.15.3",
  "bugs": {
    "url": "https://github.com/zidsa/zidmui/issues"
  },
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "build": "tsc && vite build",
    "tscheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "lint-check": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "release": "release-it"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    "./icons/arrows/*": {
      "require": "./dist/cjs/icons/arrows/*.js",
      "types": "./dist/types/icons/arrows/*.d.ts",
      "import": "./dist/es/icons/arrows/*.js"
    },
    "./icons/buildings/*": {
